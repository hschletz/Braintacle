<!--
This schema defines the XML documents generated by agents and Braintacle's
export function. These documents contain all inventory data for a client.

The 'braintacle' namespace defines additional attributes which are purely
informational and not evaluated:

- braintacle:model is used for elements below 'CONTENT'. It is the name of a
  model class (relative to the Model\Client namespace) that handles the data
  represented by the element. It is used as a default for entire inventory
  sections and can be overridden for particular child elements if the data is
  handled by a different model than the rest of the section. If this attribute
  is not present for a section, it is not used by Braintacle.

- braintacle:format is the format of date/timestamp fields, using ISO 8601
  format strings.
-->
<grammar xmlns="http://relaxng.org/ns/structure/1.0" xmlns:braintacle="https://github.com/hschletz/Braintacle">
    <start>
        <!-- Root element -->
        <element name="REQUEST">
            <interleave>
                <!-- Main inventory section. -->
                <element name="CONTENT">
                    <interleave>
                        <zeroOrMore>
                            <ref name="ACCESSLOG" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="ACCOUNTINFO" />
                        </zeroOrMore>
                        <optional>
                            <ref name="BIOS" /><!-- May be missing if all empty -->
                        </optional>
                        <zeroOrMore>
                            <ref name="CONTROLLERS" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="CPUS" />
                        </zeroOrMore>
                        <optional>
                            <ref name="DOWNLOAD" />
                        </optional>
                        <zeroOrMore>
                            <ref name="DRIVES" />
                        </zeroOrMore>
                        <ref name="HARDWARE" /><!-- Basic stuff, always present -->
                        <zeroOrMore>
                            <ref name="INPUTS" />
                        </zeroOrMore>
                        <optional>
                            <ref name="JAVAINFOS" /><!-- Provided by Android agent only -->
                        </optional>
                        <zeroOrMore>
                            <ref name="MEMORIES" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="MODEMS" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="MONITORS" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="NETWORKS" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="OFFICEPACK" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="PORTS" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="PRINTERS" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="PROCESSES" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="REGISTRY" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="SIM" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="SLOTS" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="SOFTWARES" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="SOUNDS" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="STORAGES" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="USERS" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="VIDEOS" />
                        </zeroOrMore>
                        <zeroOrMore>
                            <ref name="VIRTUALMACHINES" />
                        </zeroOrMore>
                    </interleave>
                </element>
                <!-- Additional elements -->
                <element name="DEVICEID">
                    <text />
                </element>
                <element name="QUERY">
                    <text />
                </element>
            </interleave>
        </element>
    </start>

   <!-- Inventory sections -->
    <define name="ACCESSLOG">
        <element name="ACCESSLOG"><!-- obsolete -->
            <interleave>
                <ref name="LOGDATE" />
                <ref name="USERID" />
            </interleave>
        </element>
    </define>
    <define name="ACCOUNTINFO">
        <element name="ACCOUNTINFO" braintacle:model="CustomFields">
            <interleave>
                <ref name="KEYNAME" />
                <ref name="KEYVALUE" />
            </interleave>
        </element>
    </define>
    <define name="BIOS">
        <element name="BIOS" braintacle:model="Client">
            <interleave>
                <optional>
                    <ref name="ASSETTAG" />
                </optional>
                <optional>
                    <ref name="BDATE" />
                </optional>
                <optional>
                    <ref name="BMANUFACTURER" />
                </optional>
                <optional>
                    <ref name="BVERSION" />
                </optional>
                <optional>
                    <ref name="MMANUFACTURER" />
                </optional>
                <optional>
                    <ref name="MMODEL" />
                </optional>
                <optional>
                    <ref name="MSN" />
                </optional>
                <optional>
                    <ref name="SMANUFACTURER" />
                </optional>
                <optional>
                    <ref name="SMODEL" />
                </optional>
                <optional>
                    <ref name="SSN" />
                </optional>
                <optional>
                    <ref name="TYPE" />
                </optional>
            </interleave>
        </element>
    </define>
    <define name="CONTROLLERS">
        <element name="CONTROLLERS" braintacle:model="Item\Controller">
            <interleave>
                <optional>
                    <ref name="CAPTION" />
                </optional>
                <optional>
                    <ref name="DESCRIPTION" />
                </optional>
                <optional>
                    <ref name="DRIVER" /><!-- not available in database -->
                </optional>
                <ref name="MANUFACTURER" />
                <ref name="NAME" />
                <optional>
                    <ref name="PCIID" /><!-- not available in database -->
                </optional>
                <optional>
                    <ref name="PCISLOT" /><!-- not available in database -->
                </optional>
                <optional>
                    <ref name="TYPE" />
                </optional>
                <optional>
                    <ref name="VERSION" />
                </optional>
            </interleave>
        </element>
    </define>
    <define name="CPUS">
        <element name="CPUS" braintacle:model="Cpu">
            <interleave>
                <optional>
                    <ref name="CORES" />
                </optional>
                <optional>
                    <ref name="CPUARCH" />
                </optional>
                <optional>
                    <ref name="CURRENT_SPEED" />
                </optional>
                <optional>
                    <ref name="DATA_WIDTH" />
                </optional>
                <optional>
                    <ref name="HPT" />
                </optional>
                <optional>
                    <ref name="L2CACHESIZE" />
                </optional>
                <ref name="MANUFACTURER" />
                <optional>
                    <ref name="SERIAL" /><!-- unused -->
                </optional>
                <optional>
                    <ref name="SERIALNUMBER" />
                </optional>
                <optional>
                    <ref name="SOCKET" />
                </optional>
                <optional>
                    <ref name="SPEED" />
                </optional>
                <ref name="TYPE" />
                <optional>
                    <ref name="VOLTAGE" />
                </optional>
            </interleave>
        </element>
    </define>
    <define name="DOWNLOAD">
        <element name="DOWNLOAD" braintacle:model="Client">
            <element name="HISTORY">
                <zeroOrMore>
                    <element name="PACKAGE">
                        <attribute name="ID" />
                    </element>
                </zeroOrMore>
            </element>
        </element>
    </define>
    <define name="DRIVES">
        <element name="DRIVES" braintacle:model="Item\Filesystem">
            <interleave>
                <optional>
                    <ref name="CREATEDATE" braintacle:format="yyyy/M/d HH:mm:ss" />
                </optional>
                <optional>
                    <ref name="FILESYSTEM" />
                </optional>
                <optional>
                    <ref name="FREE" />
                </optional>
                <optional>
                    <ref name="LABEL" /><!-- not available in database -->
                </optional>
                <optional>
                    <ref name="LETTER" />
                </optional>
                <optional>
                    <ref name="NUMFILES" /><!-- obsolete -->
                </optional>
                <optional>
                    <ref name="SERIAL" /><!-- not available in database -->
                </optional>
                <optional>
                    <ref name="TOTAL" />
                </optional>
                <optional>
                    <ref name="TYPE" />
                </optional>
                <optional>
                    <ref name="VOLUMN" />
                </optional>
            </interleave>
        </element>
    </define>
    <define name="HARDWARE">
        <element name="HARDWARE" braintacle:model="Client">
            <interleave>
                <optional>
                    <ref name="ARCH" />
                </optional>
                <optional>
                    <ref name="ARCHNAME" /><!-- not available in database -->
                </optional>
                <ref name="CHECKSUM" />
                <optional>
                    <ref name="DATELASTLOGGEDUSER" /><!-- not available in database -->
                </optional>
                <optional>
                    <ref name="DEFAULTGATEWAY" />
                </optional>
                <optional>
                    <ref name="DESCRIPTION" />
                </optional>
                <optional>
                    <ref name="DNS" />
                </optional>
                <optional>
                    <ref name="ETIME" /><!-- obsolete -->
                </optional>
                <optional>
                    <ref name="IPADDR" />
                </optional>
                <optional>
                    <ref name="LASTCOME" braintacle:format="yyyy-MM-dd HH:mm:ss" /><!-- UTC -->
                </optional>
                <optional>
                    <ref name="LASTDATE" braintacle:format="yyyy-MM-dd HH:mm:ss" /><!-- UTC -->
                </optional>
                <optional>
                    <ref name="LASTLOGGEDUSER" /><!-- not available in database -->
                </optional>
                <ref name="MEMORY" />
                <ref name="NAME" />
                <optional>
                    <ref name="OSCOMMENTS" />
                </optional>
                <ref name="OSNAME" />
                <optional>
                    <ref name="OSVERSION" />
                </optional>
                <optional>
                    <ref name="PROCESSORN" />
                </optional>
                <optional>
                    <ref name="PROCESSORS" />
                </optional>
                <optional>
                    <ref name="PROCESSORT" />
                </optional>
                <ref name="SWAP" />
                <optional>
                    <ref name="TYPE" /><!-- Useless, always 0 or NULL. Omitted for possible confusion with bios.type. -->
                </optional>
                <optional>
                    <ref name="USERDOMAIN" braintacle:model="WindowsInstallation" />
                </optional>
                <optional>
                    <ref name="USERID" />
                </optional>
                <optional>
                    <ref name="UUID" />
                </optional>
                <optional>
                    <ref name="VMSYSTEM" /><!-- not available in database -->
                </optional>
                <optional>
                    <ref name="WINCOMPANY" braintacle:model="WindowsInstallation" />
                </optional>
                <optional>
                    <ref name="WINOWNER" braintacle:model="WindowsInstallation" />
                </optional>
                <optional>
                    <ref name="WINPRODID" braintacle:model="WindowsInstallation" />
                </optional>
                <optional>
                    <ref name="WINPRODKEY" braintacle:model="WindowsInstallation" />
                </optional>
                <ref name="WORKGROUP" braintacle:model="WindowsInstallation" />
            </interleave>
        </element>
    </define>
    <define name="INPUTS">
        <element name="INPUTS" braintacle:model="Item\InputDevice">
            <interleave>
                <optional>
                    <ref name="CAPTION" />
                </optional>
                <optional>
                    <ref name="DESCRIPTION" />
                </optional>
                <optional>
                    <ref name="INTERFACE" />
                </optional>
                <optional>
                    <ref name="MANUFACTURER" />
                </optional>
                <optional>
                    <ref name="POINTTYPE" />
                </optional>
                <optional>
                    <ref name="TYPE" />
                </optional>
            </interleave>
        </element>
    </define>
    <define name="JAVAINFOS">
        <element name="JAVAINFOS">
            <interleave>
                <ref name="JAVACLASSPATH" />
                <ref name="JAVACOUNTRY" />
                <ref name="JAVAHOME" />
                <ref name="JAVANAME" />
                <optional>
                    <ref name="JAVAPATHLEVEL" /><!-- unused -->
                </optional>
            </interleave>
        </element>
    </define>
    <define name="MEMORIES">
        <element name="MEMORIES" braintacle:model="Item\MemorySlot">
            <interleave>
                <optional>
                    <ref name="CAPACITY" />
                </optional>
                <ref name="CAPTION" />
                <optional>
                    <ref name="DESCRIPTION" />
                </optional>
                <optional>
                    <ref name="MANUFACTURER" />
                </optional>
                <optional>
                    <ref name="NUMSLOTS" />
                </optional>
                <optional>
                    <ref name="PURPOSE" />
                </optional>
                <optional>
                    <ref name="SERIALNUMBER" />
                </optional>
                <optional>
                    <ref name="SPEED" />
                </optional>
                <ref name="TYPE" />
            </interleave>
        </element>
    </define>
    <define name="MODEMS">
        <element name="MODEMS" braintacle:model="Item\Modem">
            <interleave>
                <ref name="NAME" />
                <ref name="TYPE" />
                <optional>
                    <ref name="DESCRIPTION" /><!-- unused -->
                </optional>
                <optional>
                    <ref name="MODEL" /><!-- unused -->
                </optional>
            </interleave>
        </element>
    </define>
    <define name="MONITORS">
        <element name="MONITORS" braintacle:model="Item\Display">
            <interleave>
                <optional>
                    <ref name="BASE64" /><!-- not available in database -->
                </optional>
                <optional>
                    <ref name="CAPTION" />
                </optional>
                <optional>
                    <ref name="DESCRIPTION" />
                </optional>
                <optional>
                    <ref name="MANUFACTURER" />
                </optional>
                <optional>
                    <ref name="SERIAL" />
                </optional>
                <optional>
                    <ref name="TYPE" />
                </optional>
                <optional>
                    <ref name="UUENCODE" /><!-- not available in database -->
                </optional>
            </interleave>
        </element>
    </define>
    <define name="NETWORKS">
        <element name="NETWORKS" braintacle:model="Item\NetworkInterface">
            <interleave>
                <ref name="DESCRIPTION" />
                <optional>
                    <ref name="DRIVER" /><!-- not available in database -->
                </optional>
                <optional>
                    <ref name="IPADDRESS" />
                </optional>
                <optional>
                    <ref name="IPDHCP" />
                </optional>
                <optional>
                    <ref name="IPGATEWAY" />
                </optional>
                <optional>
                    <ref name="IPMASK" />
                </optional>
                <optional>
                    <ref name="IPSUBNET" />
                </optional>
                <ref name="MACADDR" />
                <optional>
                    <ref name="PCISLOT" /><!-- not available in database -->
                </optional>
                <optional>
                    <ref name="SPEED" />
                </optional>
                <ref name="STATUS" />
                <ref name="TYPE" />
                <optional>
                    <ref name="TYPEMIB" />
                </optional>
                <optional>
                    <ref name="VIRTUALDEV" />
                </optional>
            </interleave>
        </element>
    </define>
    <define name="OFFICEPACK">
        <element name="OFFICEPACK" braintacle:model="Item\MsOfficeProduct">
            <interleave>
                <optional>
                    <ref name="GUID" />
                </optional>
                <ref name="INSTALL" />
                <optional>
                    <ref name="NOTE" />
                </optional>
                <optional>
                    <ref name="OFFICEKEY" />
                </optional>
                <ref name="OFFICEVERSION" />
                <ref name="PRODUCT" />
                <optional>
                    <ref name="PRODUCTID" />
                </optional>
                <ref name="TYPE" />
            </interleave>
        </element>
    </define>
    <define name="PORTS">
        <element name="PORTS" braintacle:model="Item\Port">
            <interleave>
                <optional>
                    <ref name="CAPTION" />
