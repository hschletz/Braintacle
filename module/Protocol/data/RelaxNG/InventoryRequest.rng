                <optional>
                    <ref name="DESCRIPTION" />
                </optional>
                <optional>
                    <ref name="NAME" />
                </optional>
                <optional>
                    <ref name="TYPE" />
                </optional>
            </interleave>
        </element>
    </define>
    <define name="PRINTERS">
        <element name="PRINTERS" braintacle:model="Item\Printer">
            <interleave>
                <optional>
                    <ref name="DESCRIPTION" />
                </optional>
                <optional>
                    <ref name="DRIVER" />
                </optional>
                <ref name="NAME" />
                <optional>
                    <ref name="PORT" />
                </optional>
            </interleave>
        </element>
    </define>
    <define name="PROCESSES">
        <element name="PROCESSES"><!-- not available in database -->
            <interleave>
                <ref name="CMD" />
                <ref name="CPUUSAGE" />
                <ref name="MEM" />
                <ref name="PID" />
                <ref name="STARTED" />
                <ref name="TTY" />
                <ref name="USER" />
                <ref name="VIRTUALMEMORY" />
            </interleave>
        </element>
    </define>
    <define name="REGISTRY">
        <element name="REGISTRY" braintacle:model="Item\RegistryData">
            <interleave>
                <ref name="NAME" />
                <ref name="REGVALUE" />
            </interleave>
        </element>
    </define>
    <define name="SIM">
        <element name="SIM" braintacle:model="Item\Sim">
            <interleave>
                <ref name="COUNTRY" />
                <ref name="DEVICEID" />
                <ref name="OPERATOR" />
                <ref name="OPNAME" />
                <ref name="SERIALNUMBER" />
            </interleave>
        </element>
    </define>
    <define name="SLOTS">
        <element name="SLOTS" braintacle:model="Item\ExtensionSlot">
            <interleave>
                <ref name="DESCRIPTION" />
                <optional>
                    <ref name="DESIGNATION" />
                </optional>
                <ref name="NAME" />
                <optional>
                    <ref name="PSHARE" /><!-- obsolete -->
                </optional>
                <optional>
                    <ref name="PURPOSE" />
                </optional>
                <optional>
                    <ref name="STATUS" />
                </optional>
            </interleave>
        </element>
    </define>
    <define name="SOFTWARES">
        <element name="SOFTWARES" braintacle:model="Item\Software">
            <interleave>
                <optional>
                    <ref name="BITSWIDTH" />
                </optional>
                <optional>
                    <ref name="COMMENTS" />
                </optional>
                <optional>
                    <ref name="FILENAME" />
                </optional>
                <optional>
                    <ref name="FILESIZE" />
                </optional>
                <optional>
                    <ref name="FOLDER" />
                </optional>
                <optional>
                    <ref name="FROM" /><!-- not available in database -->
                </optional>
                <optional>
                    <ref name="GUID" />
                </optional>
                <optional>
                    <ref name="INSTALLDATE" braintacle:format="yyyy/MM/dd" />
                </optional>
                <optional>
                    <ref name="LANGUAGE" />
                </optional>
                <optional>
                    <ref name="NAME" />
                </optional>
                <optional>
                    <ref name="PUBLISHER" />
                </optional>
                <optional>
                    <ref name="SOURCE" />
                </optional>
                <optional>
                    <ref name="VERSION" />
                </optional>
            </interleave>
        </element>
    </define>
    <define name="SOUNDS">
        <element name="SOUNDS" braintacle:model="Item\AudioDevice">
            <interleave>
                <optional>
                    <ref name="DESCRIPTION" />
                </optional>
                <ref name="MANUFACTURER" />
                <ref name="NAME" />
            </interleave>
        </element>
    </define>
    <define name="STORAGES">
        <element name="STORAGES" braintacle:model="Item\StorageDevice">
            <interleave>
                <optional>
                    <ref name="DESCRIPTION" />
                </optional>
                <optional>
                    <ref name="DISKSIZE" />
                </optional>
                <optional>
                    <ref name="FIRMWARE" />
                </optional>
                <optional>
                    <ref name="MANUFACTURER" />
                </optional>
                <optional>
                    <ref name="MODEL" />
                </optional>
                <optional>
                    <ref name="NAME" />
                </optional>
                <optional>
                    <ref name="SCSI_CHID" /><!-- not available in database -->
                </optional>
                <optional>
                    <ref name="SCSI_COID" /><!-- not available in database -->
                </optional>
                <optional>
                    <ref name="SCSI_LUN" /><!-- not available in database -->
                </optional>
                <optional>
                    <ref name="SCSI_UNID" /><!-- not available in database -->
                </optional>
                <optional>
                    <ref name="SERIALNUMBER" />
                </optional>
                <optional>
                    <ref name="TYPE" />
                </optional>
            </interleave>
        </element>
    </define>
    <define name="USERS">
        <element name="USERS"><!-- not available in database -->
            <ref name="LOGIN" />
        </element>
    </define>
    <define name="VIDEOS">
        <element name="VIDEOS" braintacle:model="Item\DisplayController">
            <interleave>
                <optional>
                    <ref name="CHIPSET" />
                </optional>
                <optional>
                    <ref name="MEMORY" />
                </optional>
                <ref name="NAME" />
                <optional>
                    <ref name="RESOLUTION" />
                </optional>
            </interleave>
        </element>
    </define>
    <define name="VIRTUALMACHINES">
        <element name="VIRTUALMACHINES" braintacle:model="Item\VirtualMachine">
            <interleave>
                <optional>
                    <ref name="MEMORY" />
                </optional>
                <ref name="NAME" />
                <ref name="STATUS" />
                <ref name="SUBSYSTEM" />
                <ref name="UUID" />
                <ref name="VCPU" />
                <optional>
                    <ref name="VMID" /><!-- not available in database -->
                </optional>
                <ref name="VMTYPE" />
            </interleave>
        </element>
    </define>

   <!-- Text elements -->
    <define name="ARCH">
