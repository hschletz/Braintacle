{
    "name": "hardware",
    "comment": "Clients and groups",
    "columns": [
        {
            "name": "id",
            "type": "integer",
            "length": 32,
            "notnull": true,
            "default": null,
            "autoincrement": true,
            "comment": null
        },
        {
            "name": "deviceid",
            "type": "varchar",
            "length": 255,
            "notnull": true,
            "default": null,
            "autoincrement": false,
            "comment": "Client-generated uniqe ID. Groups have special values __SYSTEMGROUP__ and __DOWNLOADGROUP__."
        },
        {
            "name": "uuid",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "System UUID reported by agent"
        },
        {
            "name": "name",
            "type": "varchar",
            "length": 255,
            "notnull": true,
            "default": null,
            "autoincrement": false,
            "comment": "Client or group name"
        },
        {
            "name": "workgroup",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "Windows workgroup/domain"
        },
        {
            "name": "userid",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "Name of active user"
        },
        {
            "name": "userdomain",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "Windows domain of active user (computer name for local account)"
        },
        {
            "name": "osname",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "OS name reported by agent"
        },
        {
            "name": "osversion",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "OS version number"
        },
        {
            "name": "oscomments",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "OS specific version string: service pack (Windows), kernel version (Linux)..."
        },
        {
            "name": "description",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "OS comment for clients, description for groups"
        },
        {
            "name": "wincompany",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "Company name entered at Windows setup"
        },
        {
            "name": "winowner",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "Owner name entered at Windows setup"
        },
        {
            "name": "winprodid",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "Windows Product ID"
        },
        {
            "name": "winprodkey",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "Windows product key (reported by agent)"
        },
        {
            "name": "arch",
            "type": "varchar",
            "length": 30,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "Windows CPU architecture"
        },
        {
            "name": "processort",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "CPU type"
        },
        {
            "name": "processors",
            "type": "integer",
            "length": 32,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "CPU clock in MHz"
        },
        {
            "name": "processorn",
            "type": "integer",
            "length": 16,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "Amount of CPU cores"
        },
        {
            "name": "memory",
            "type": "integer",
            "length": 32,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "Maximum amount of RAM avalilable to OS (in MB). May be lower than physical RAM."
        },
        {
            "name": "swap",
            "type": "integer",
            "length": 32,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "Amount of swap space (in MB)"
        },
        {
            "name": "ipaddr",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "deprecated"
        },
        {
            "name": "ipsrc",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "deprecated"
        },
        {
            "name": "dns",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "IPv4 address of primary DNS server (UNIX agent only)"
        },
        {
            "name": "defaultgateway",
            "type": "varchar",
            "length": 255,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "IPv4 address of default gateway (UNIX agent only)"
        },
        {
            "name": "etime",
            "type": "integer",
            "length": 32,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "deprecated"
        },
        {
            "name": "lastdate",
            "type": "timestamp",
            "length": null,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "Timestamp of last inventory"
        },
        {
            "name": "lastcome",
            "type": "timestamp",
            "length": null,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "Timestamp of last agent contact"
        },
        {
            "name": "useragent",
            "type": "varchar",
            "length": 50,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "Agent ID sting"
        },
        {
            "name": "quality",
            "type": "decimal",
            "length": "7,4",
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "deprecated"
        },
        {
            "name": "fidelity",
            "type": "integer",
            "length": 64,
            "notnull": false,
            "default": 1,
            "autoincrement": false,
            "comment": "deprecated"
        },
        {
            "name": "type",
            "type": "integer",
            "length": 32,
            "notnull": false,
            "default": null,
            "autoincrement": false,
            "comment": "deprecated"
        },
        {
            "name": "checksum",
            "type": "integer",
            "length": 64,
            "notnull": false,
            "default": 262143,
            "autoincrement": false,
            "comment": null
        }
    ],
    "primary_key": [
        "id"
    ],
    "indexes": [
        {
            "name": "hardware_deviceid_idx",
            "unique": false,
            "columns": [
                "deviceid"
            ]
        },
        {
            "name": "hardware_name_idx",
            "unique": false,
            "columns": [
                "name"
            ]
        },
        {
            "name": "hardware_memory_idx",
            "unique": false,
            "columns": [
                "memory"
            ]
        },
        {
            "name": "hardware_osname_idx",
            "unique": false,
            "columns": [
                "osname"
            ]
        },
        {
            "name": "hardware_userid_idx",
            "unique": false,
            "columns": [
                "userid"
            ]
        },
        {
            "name": "hardware_workgroup_idx",
            "unique": false,
            "columns": [
                "workgroup"
            ]
        },
        {
            "name": "hardware_checksum_idx",
            "unique": false,
            "columns": [
                "checksum"
            ]
        },
        {
            "name": "hardware_name_clientid_unique_idx",
            "unique": true,
            "columns": [
                "name",
                "deviceid"
            ]
        }
    ],
    "mysql": {
