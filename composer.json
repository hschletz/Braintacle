{
    "name": "hschletz/braintacle",
    "description": "A set of applications for managing hard- and software on a network",
    "homepage": "https://hschletz.github.io/Braintacle",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Holger Schletz",
            "email": "holger.schletz@web.de"
        }
    ],
    "support": {
        "issues": "https://github.com/hschletz/Braintacle/issues"
    },
    "autoload": {
        "files": [
            "module/Library/Application.php"
        ]
    },
    "require": {
        "php": ">=7.0",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-zip": "*",
        "components/jquery": "^3.0",
        "hschletz/nada": "^1.1.4",
        "oomphinc/composer-installers-extender": "^2.0",
        "symfony/filesystem": "^5.2",
        "theseer/fdomdocument": "*",
        "zendframework/zend-authentication": "^2.5",
        "zendframework/zend-config": "^2.5 || ^3.0",
        "zendframework/zend-console": "^2.5",
        "zendframework/zend-db": "^2.9",
        "zendframework/zend-debug": "^2.5",
        "zendframework/zend-eventmanager": "^3.0",
        "zendframework/zend-filter": "^2.7",
        "zendframework/zend-form": "^2.11",
        "zendframework/zend-http": "^2.5",
        "zendframework/zend-hydrator": "^2.2",
        "zendframework/zend-i18n": "^2.7",
        "zendframework/zend-i18n-resources": "^2.5",
        "zendframework/zend-inputfilter": "^2.8",
        "zendframework/zend-loader": "^2.5",
        "zendframework/zend-log": "^2.9",
        "zendframework/zend-math": "^3.0",
        "zendframework/zend-modulemanager": "^2.5",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-mvc-form": "^1.0",
        "zendframework/zend-mvc-i18n": "^1.0",
        "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
        "zendframework/zend-navigation": "^2.8",
        "zendframework/zend-router": "^3.0",
        "zendframework/zend-servicemanager": "^3.1",
        "zendframework/zend-session": "^2.5",
        "zendframework/zend-stdlib": "^3.0",
        "zendframework/zend-uri": "^2.5",
        "zendframework/zend-validator": "^2.8",
        "zendframework/zend-view": "^2.5",
        "zfcampus/zf-console": "^1.3"
    },
    "require-dev": {
        "hschletz/doxygen-phpdoc": "^1.0",
        "kornrunner/dbunit": "^6.0",
        "mikey179/vfsstream": "*",
        "phpunit/phpunit": "^8",
        "sebastian/peek-and-poke": "1.0.*-dev",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/process": "^5.2",
        "zendframework/zend-dom": "^2.5",
        "zendframework/zend-test": "^3.0"
    },
    "suggest": {
        "ext-zlib": "Required for 'braintacle-tool.php decode'",
        "ext-gmp": "Required for PHP installations without 64 bit integer support"
    },
    "bin": [
        "braintacle-tool.php"
    ],
    "extra": {
        "installer-types": ["component"],
        "installer-paths": {
            "public/components/{$name}": ["type:component"]
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.3.23"
        }
    }
}
