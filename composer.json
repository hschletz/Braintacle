{
    "name": "hschletz/braintacle",
    "description": "A set of applications for managing hard- and software on a network",
    "homepage": "https://hschletz.github.io/Braintacle",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Holger Schletz",
            "email": "holger.schletz@web.de"
        }
    ],
    "support": {
        "issues": "https://github.com/hschletz/Braintacle/issues"
    },
    "autoload": {
        "files": [
            "module/Library/Application.php"
        ]
    },
    "require": {
        "php": "^7.3",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-zip": "*",
        "components/jquery": "^3.0",
        "hschletz/nada": "^1.1.4",
        "laminas/laminas-authentication": "^2.5",
        "laminas/laminas-config": "^3.0",
        "laminas/laminas-console": "^2.5",
        "laminas/laminas-db": "^2.9",
        "laminas/laminas-eventmanager": "^3.0",
        "laminas/laminas-filter": "^2.7",
        "laminas/laminas-form": "^2.11",
        "laminas/laminas-http": "^2.5",
        "laminas/laminas-hydrator": "^3.0",
        "laminas/laminas-i18n": "^2.7",
        "laminas/laminas-i18n-resources": "^2.5",
        "laminas/laminas-inputfilter": "^2.8",
        "laminas/laminas-loader": "^2.5",
        "laminas/laminas-log": "^2.9",
        "laminas/laminas-math": "^3.0",
        "laminas/laminas-modulemanager": "^2.5",
        "laminas/laminas-mvc": "^3.0",
        "laminas/laminas-mvc-form": "^1.0",
        "laminas/laminas-mvc-i18n": "^1.0",
        "laminas/laminas-mvc-plugin-flashmessenger": "^1.0",
        "laminas/laminas-navigation": "^2.8",
        "laminas/laminas-router": "^3.0",
        "laminas/laminas-servicemanager": "^3.1",
        "laminas/laminas-session": "^2.5",
        "laminas/laminas-stdlib": "^3.0",
        "laminas/laminas-uri": "^2.5",
        "laminas/laminas-validator": "^2.8",
        "laminas/laminas-view": "^2.5",
        "oomphinc/composer-installers-extender": "^2.0",
        "symfony/console": "^5.2",
        "symfony/event-dispatcher": "^5.2",
        "symfony/filesystem": "^5.2",
        "symfony/var-dumper": "^5.2",
        "theseer/fdomdocument": "^1.6"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.3",
        "kornrunner/dbunit": "^6.0",
        "laminas/laminas-dom": "^2.5",
        "laminas/laminas-test": "^3.0",
        "mikey179/vfsstream": "*",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/process": "^5.2"
    },
    "suggest": {
        "ext-zlib": "Required for 'braintacle-tool.php decode'",
        "ext-gmp": "Required for PHP installations without 64 bit integer support"
    },
    "bin": [
        "braintacle-tool.php"
    ],
    "extra": {
        "installer-types": ["component"],
        "installer-paths": {
            "public/components/{$name}": ["type:component"]
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.3.23"
        }
    }
}
